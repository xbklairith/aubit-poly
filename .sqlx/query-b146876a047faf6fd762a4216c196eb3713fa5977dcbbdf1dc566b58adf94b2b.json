  "query": "\n        SELECT\n            id,\n            condition_id,\n            market_type,\n            asset,\n            timeframe,\n            yes_token_id,\n            no_token_id,\n            name,\n            end_time,\n            COALESCE(is_active, true) as \"is_active!\",\n            COALESCE(discovered_at, NOW()) as \"discovered_at!\",\n            COALESCE(updated_at, NOW()) as \"updated_at!\"\n        FROM markets\n        WHERE condition_id = $1\n        ",
