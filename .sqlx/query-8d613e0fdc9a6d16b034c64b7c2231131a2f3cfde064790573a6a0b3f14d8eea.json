  "query": "\n        SELECT\n            id,\n            market_id,\n            yes_shares,\n            no_shares,\n            total_invested,\n            COALESCE(status, 'open') as \"status!\",\n            COALESCE(is_dry_run, true) as \"is_dry_run!\",\n            COALESCE(opened_at, NOW()) as \"opened_at!\",\n            closed_at\n        FROM positions\n        WHERE status = 'open'\n        ORDER BY opened_at DESC\n        ",
